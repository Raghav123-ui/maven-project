pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
       }
       stage('deploy tomcat')
       {
       steps
       {
       sshagent(['deploy-dev']) {
       
       sh 'scp -o stricthostkeychecking=no /var/lib/jenkins/workspace/sonar_cube/webapp.war ec2-user@3.135.17.162:8080:/var/lib/tomcat/webapps/'
       }}}}
}
