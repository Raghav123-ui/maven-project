pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
       }
       stage('deploy tomcat')
       {
       steps
       {
       sshagent(['32dba8e9-d51b-4380-8891-bbf0474e4aae']) {
       
       sh 'scp -o stricthostkeychecking=no /var/lib/jenkins/workspace/job/target/webapp.war ec2-user@172.31.27.16:8080:/var/lib/tomcat/webapps/'
       }}}}
}

