pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
       }
       stage('sonar install')
       {
       steps
       {
       withSonarQubeEnv('sonar5') {
       
       withmaven(jdk: 'localjdk 1.8',maven: 'localmaven')
       {
       sh 'mvn install sonar:sonar'}
       }}}}
}
