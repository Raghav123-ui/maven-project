pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
       }
       stage('deploy tomcat')
       {
       steps
       {
       sshagent(['36c73152-b372-43ae-abbb-20bf2ef0a392']) {
       
       sh 'scp -o stricthostkeychecking=no */target/*.war ec2-user@3.136.83.79:8080:/var/lib/tomcat/webapps
       }
       }}}}
}

