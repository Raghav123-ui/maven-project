pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
       }
       stage
       {
       steps
       {
       withSonarQubeEnv(credentialsId: 's1') {
       
       withmaven(jdk: 'localjdk 1.8',maven: 'localmaven')
       {
       sh 'clean mvn install sonar:sonar'}
       }}}}
}
