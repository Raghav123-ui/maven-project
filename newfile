pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
               }
       parallel createpackage:{
       stage(maven package goal'){
       sh 'mvn package'
       }
       },installm2repo: {
       stage('maven install goal')
       {
       sh 'mvn install'
       }
       },
       failfast:true     
}}
