pipeline
{
   agent any
   stages
    {
     stage('scm checkout')
      {
        steps {

          git branch: 'master',url: 'https://github.com/Raghav123-ui/maven-project.git'
               }
       }
       stage('parallel stage')
       parallel{
       steps
       {
       stage('compile stage')
       {
       steps {
       withmaven(maven: 'localmaven'){
        sh 'man compile'
     }}
     {
     stage('test'){
     steps{
    withmaven(maven: 'localmaven') {
    sh 'man test'
    }}}}}}}
    failfast:true
}
